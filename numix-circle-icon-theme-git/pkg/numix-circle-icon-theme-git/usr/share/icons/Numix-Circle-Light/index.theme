[Icon Theme]
Name=Numix Circle Light
Comment=Numix icon theme with a circular apps icons design for use with light themes

Inherits=Numix-Light

Example=directory-x-normal

# Directory list
Directories=48x48/apps,scalable/apps

[48x48/apps]
Size=48
Context=Applications
Type=Fixed

[scalable/apps]
Size=16
Context=Applications
MinSize=8
MaxSize=512
Type=Scalable
